# Generated by wsman, do not touch it!

{% for db in databases %}WS_DB_{{ db.confname }}_NAME={{ db.dbname }}
WS_DB_{{ db.confname }}_USER={{ db.username }}
WS_DB_{{ db.confname }}_PASS={{ db.password }}
{% endfor %}
{% for k, v in extra_hosts %}WS_HOSTNAME_{{ k }}={{ v }}
{% endfor %}
{% if solr_enabled %}
{% if solr_container_ip %}
WS_SOLR_HOST={{ solr_container_ip }}
{% endif %}
{% if solr_cores %}
{% for solr_core in solr_cores %}
WS_SOLR_CORE_{{ solr_core.confname }}_NAME={{ solr_core.corename }}
{% endfor %}
{% endif %}
{% endif %}
